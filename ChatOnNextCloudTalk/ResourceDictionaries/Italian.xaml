<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ChatOnNextCloudTalk"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- main window-->
    <system:String x:Key="login">ACCEDI</system:String>
    <system:String x:Key="savePassword">Salva password</system:String>

    <!-- chatview -->
    <system:String x:Key="searchbar">Cerca...</system:String>
    <system:String x:Key="messagebar">Scrivi un messaggio</system:String>

    <!-- message boxes-->
    <!-- MainWindowVM.cs -->
    <!-- if User == null -->
    <system:String x:Key="mb3Text">Dati inseriti non validi.</system:String>
    <system:String x:Key="mb3Title">Errore</system:String>

    <!-- avatar details -->
    <system:String x:Key="avatarTitle">Immagine del profilo</system:String>
    <system:String x:Key="loadButton">Carica un'immagine...</system:String>
    <system:String x:Key="delButton">Elimina immagine</system:String>
    <!-- vm -->
    <system:String x:Key="dialogTitle">Seleziona un'immagine</system:String>

    <!-- GENERICS -->
    <system:String x:Key="cancelButton">Annulla</system:String>

</ResourceDictionary>